{
  "data": {
    "input_csv": "data/AI销售_20260123_184242.csv",
    "text_columns": ["question", "answer"],
    "id_column": "ID"
  },
  "clustering": {
    "n_clusters": 2,
    "batch_size": 5000,
    "max_iter": 50,
    "random_state": 42
  },
  "qwen": {
    "api_key": "sk-79959346b3674d788c6d5ac43f4eb905",
    "base_url": "https://dashscope.aliyuncs.com/compatible-mode/v1",
    "model": "qwen-flash",
    "max_retries": 3,
    "timeout": 30
  },
  "document_processing": {
    "chunk_size": 1000,
    "chunk_overlap": 350,
    "max_qa_pairs_per_chunk": 3,
    "max_content_length": 4000,
    "output_csv": "data/processed_qa_pairs.csv",
    "temp_dir": "temp",
    "supported_extensions": [".txt", ".pdf", ".docx", ".doc", ".md", ".html", ".csv", ".epub", ".ppt", ".pptx", ".odt", ".eml", ".enex"],
    "enable_qa_validation": true,
    "validation_batch_size": 10,
    "header_lines": 3,
    "footer_lines": 3
  },
  "trace": {
    "enable_tracing": true,
    "trace_dir": "data/trace_data",
    "sample_size": 5,
    "stages": {
      "raw_documents": true,
      "cleaned_documents": true,
      "merged_documents": true,
      "text_chunks": true,
      "generated_qa": true,
      "validated_qa": true
    }
  },
  "clip": {
    "model_name": "paraphrase-multilingual-MiniLM-L12-v2",
    "batch_size": 128,
    "device": "cuda"
  },
  "milvus": {
    "host": "milvus-standalone",
    "port": 19530,
    "collection_name": "multilingual_vectors",
    "dimension": 384,
    "index_type": "IVF_FLAT",
    "metric_type": "COSINE",
    "nlist": 1024,
    "nprobe": 64,
    "milvus_config_path": "tool_configs/milvus_config.json"
  },
  "service": {
    "host": "0.0.0.0",
    "port": 8000,
    "top_k": 10
  },
  "performance": {
    "enable_metrics": true,
    "gpu_sample_interval": 1.0,
    "report_format": ["json", "html"]
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}
